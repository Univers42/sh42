FOO=bar; echo "$FOO"
export FOO=bar; echo "$FOO"
FOO=bar; export FOO; echo "$FOO"
export FOO ; unset FOO; echo "$FOO"
A=1; B=2; C=3; export A B C; echo "$A $B $C"
export A=ok && echo "$A"
export B=ok && false && echo "will not print"
export C=ok || echo "will not print"
false || export D=fallback; echo "$D"
export S=parent; ( S=child ); echo "$S"
export S=parent; (export S=child); echo "$S"
export VAR=before; VAR=after; echo "$VAR"
export PARENT=visible; echo "$PARENT"; ( unset PARENT; echo "$PARENT" ); echo "$PARENT"
export MULTI=one two three; echo "$MULTI"
export EMPTY=; echo ">$EMPTY<"
export NUM=123; echo "$NUM"
export SPECIAL=!@#$%^&*()_+; echo "$SPECIAL"
export QUOTE="with spaces"; echo "$QUOTE"
export EX=works; echo "$EX"
export PIPE=test | cat
echo test | export BAD=idea; echo "$BAD"
export GOOD=idea; echo test | cat; echo "$GOOD"
export VAR1=val1 VAR2=val2; echo "$VAR1 $VAR2"
export ENV1=hello; env | grep ENV1
A=1 ; echo "$A" && echo "ok" || echo "fail"
export OVERRIDE=global; OVERRIDE=local; echo "$OVERRIDE"
echo "$OVERRIDE"
export RO=locked; readonly RO; RO=newvalue; echo "$RO"
RO=fail || echo "cannot modify readonly variable"
echo "$PATH"
export PATH="/tmp:$PATH"; command -v ls
